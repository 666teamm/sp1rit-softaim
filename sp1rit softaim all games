-- SP1RIT UPDATE
-- Version: 3.2

-- Instances:

local indexfree = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local TextLabel = Instance.new("TextLabel")

--Properties:

indexfree.Name = "index  [free]"
indexfree.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
indexfree.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Frame.Parent = indexfree
Frame.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
Frame.BorderColor3 = Color3.fromRGB(255, 255, 255)
Frame.Position = UDim2.new(0.205529466, 0, 0.11818856, 0)
Frame.Size = UDim2.new(0, 626, 0, 420)

TextLabel.Parent = Frame
TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
TextLabel.BackgroundTransparency = 0.990
TextLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
TextLabel.BorderSizePixel = 0
TextLabel.Position = UDim2.new(0.154952079, 0, 0.330952317, 0)
TextLabel.Size = UDim2.new(0, 432, 0, 142)
TextLabel.Font = Enum.Font.Unknown
TextLabel.Text = "PLZ WAIT UPDATING..."
TextLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
TextLabel.TextSize = 21.000

-- Scripts:

local function TBRZPMN_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	
	local UserInputService = game:GetService("UserInputService")
	
	function onKeyPress(inputObject, gameProcessedEvent)
		if not gameProcessedEvent then
			if inputObject.KeyCode == Enum.KeyCode.Insert then 
				if script.Parent.Parent.Frame.Visible == false then
					script.Parent.Parent.Frame.Visible = true
				else
					script.Parent.Parent.Frame.Visible = false
				end
			end
		end
	end
	
	UserInputService.InputBegan:connect(onKeyPress)
	local Frame = script.Parent
end
coroutine.wrap(TBRZPMN_fake_script)()
local function RDFMQQM_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	Frame = script.Parent.Frame
	
	Frame.Draggable = true
	Frame.Selectable = true
	Frame.Active = true
	
end
coroutine.wrap(RDFMQQM_fake_script)()
